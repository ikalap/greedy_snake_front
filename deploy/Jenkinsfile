pipeline {
    agent any

    stages {
        stage('Build & Deploy') {
            steps {
                sh 'ls'
//                 sh 'docker-compose -f deploy/docker-compose.yml up -d'
                sh 'docker build -t snake-front --no-cache ./deploy'
            }
        }

        stage('Finish') {
            steps {
                sh 'echo "部署成功"了'
            }
        }
    }
}
